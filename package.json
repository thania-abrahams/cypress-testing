    "cypress": "^11.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-real-events": "^1.7.4",
    "mocha": "^10.1.0",
    "mochawesome": "^7.1.3",
    "mysql": "mysqljs/mysql"
  }
